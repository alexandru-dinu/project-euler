   "source": [
    "def is_palindrome(n):\n",
    "    x1 = str(n)\n",
    "    x2 = bin(n)[2:]\n",
    "    return (x1 == x1[::-1]) and (x2 == x2[::-1])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "%%time\n",
    "N = 1e6\n",
    "s = [n for n in range(int(N)) if is_palindrome(n)]\n",
    "sum(s)"
   ]
  }
 ],
