   "source": [
    "def gen():\n",
    "    l = 1\n",
    "    while True:\n",
    "        n = sum(range(1, l+1))\n",
    "        d = len(divisors(n))\n",
    "        yield n, d\n",
    "        l += 1\n",
    "        \n",
    "g = gen()\n",
    "\n",
    "while True:\n",
    "    n, d = next(g)\n",
    "    if d > 500:\n",
    "        print(n)\n",
    "        break"
   ]
  }
